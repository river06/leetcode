package HW;

import java.util.List;

public class Solution {
	public int minimumTotal(List<List<Integer>> triangle) {
		
		if( triangle.isEmpty() ) {
			return 0;
		}
        int height = triangle.size();
		if( height == 1 ) return triangle.get(0).get(0);
		int width = height;
		
		
		return 0;
    }
}
